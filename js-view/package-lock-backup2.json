{
  "name": "js-view",
  "displayName": "JS View",
  "description": "A VS Code extension to uglify and beautify JavaScript",
  "version": "0.0.1",
  "publisher": "your-publisher-name",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.uglifyJs",
    "onCommand:extension.beautifyJs"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.uglifyJs",
        "title": "Uglify JavaScript"
      },
      {
        "command": "extension.beautifyJs",
        "title": "Beautify JavaScript"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.50.0",
    "typescript": "^4.0.3",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "uglify-js": "^3.13.10",
    "prettier": "^2.3.2"
  }
}
